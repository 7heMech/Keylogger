{
  "name": "Webhook Tester",
  "version": "0.2.9",
  "manifest_version": 3,
  "description": "Test webhooks.",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "inject.js"
  },
  "host_permissions": ["*://*/*"],
  "permissions": [
    "tabs",
    "storage",
    "scripting"
  ]
}